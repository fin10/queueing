{
  "name": "queueing",
  "author": "fender64@gmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "prebuild": "rimraf dist",
    "build": "yarn build:client && yarn build:server",
    "build:client": "parcel build ./client/index.html -d ./dist/client",
    "build:server": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "concurrently \"yarn watch:client\" \"nest start\"",
    "start:dev": "concurrently \"yarn watch:client\" \"nest start --watch\"",
    "start:debug": "concurrently \"yarn watch:client\" \"nest start --debug --watch\"",
    "start:prod": "node dist/main",
    "watch:client": "parcel watch ./client/index.html -d ./dist/client",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@fluentui/react": "^7.154.0",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/mongoose": "^7.2.0",
    "@nestjs/platform-express": "^7.5.1",
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "mongoose": "^5.11.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^5.0.4",
    "jest": "^26.6.3",
    "lodash-es": "^4.17.15",
    "parcel": "^1.12.4",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
