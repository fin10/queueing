{
  "name": "queueing",
  "author": "fender64@gmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "prebuild": "rimraf dist",
    "build": "yarn build:client && yarn build:server",
    "build:client": "parcel build ./client/index.html -d ./dist/client",
    "build:server": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "concurrently \"yarn watch:client\" \"nest start\"",
    "start:debug": "concurrently \"yarn watch:client\" \"nest start --debug --watch\"",
    "start:prod": "node dist/apps/webservice/main.js",
    "watch:client": "parcel watch ./client/index.html -d ./dist/client",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/webservice/test/jest-e2e.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@nestjs/common": "^7.5.1",
